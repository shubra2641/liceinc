{
    "name": "my-logos/email-services",
    "description": "Modular email services system for Laravel",
    "type": "library",
    "version": "1.0.0",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/mail": "^10.0",
        "illuminate/database": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Services\\Email\\": "."
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "App\\Services\\Email\\EmailServiceProvider"
            ],
            "aliases": {
                "Email": "App\\Services\\Email\\Facades\\Email"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}